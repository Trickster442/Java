package com.lab2;

public class Ex2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double age = 20 ;
		if (age > 5 & age < 12) {
			System.out.println("You are a child");
			
		} else if(age >= 3 & age <= 5) {
			System.out.println("You're a toddler");
		}
	}

}
